/* ftcomponents/ftCustomNav/ftCustomNav.wxss */
@import "../../app.wxss";

.wrapper-normal{
  width: 100%;
}
.wrapper-fixed{
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
}
.nav-box{
  width: 100%;
  height: 44px;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.left-btns{
  position: absolute;
  left: 0;
  top: 0;
  height: 100%;
  overflow: hidden;
  padding-left: 7px;
  display: flex;
  align-items: center;
}
.nav-back{
  font-size: 20px;
  margin: -1rpx 0 0 -1rpx;
}
.title{
  font-size: 18px;
  line-height: 24px;
  font-weight: 500;
  padding-bottom: 2px;
}