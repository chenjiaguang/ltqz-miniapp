@font-face {
    font-family: 'lutufont';
    src: local('lutufont'),
        url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('lutufont.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}

.lutufont {
  font-family: "lutufont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.lutu-jian:before {
  content: "\e68a";
}

.lutu-jia:before {
  content: "\e61f";
}

.lutu-xinghao:before {
  content: "\e64d";
}

.lutu-next:before {
  content: "\e661";
}

.lutu-location:before {
  content: "\e690";
}