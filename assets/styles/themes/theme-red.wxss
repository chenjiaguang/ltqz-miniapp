.theme-linear-gradient-h{
  background-image: linear-gradient(to right, #FF5433, #FF2266) !important;
}
.theme-linear-gradient-secondary-h{
  background-image: linear-gradient(to right, #F60493, #CD57EB) !important;
}
.theme-linear-gradient-h-reverse{
