}
.theme-linear-gradient-v{
  background-image: linear-gradient(to bottom, #FF5433, #FF2266) !important;
}
/* 170deg的渐变 */
.theme-linear-gradient-170{
  background-image: linear-gradient(170deg, #FF5433, #FF2266) !important;
}
/* 170deg的反向渐变 */
.theme-linear-gradient-170-reverse{
  background-image: linear-gradient(170deg, #FF2266, #FF5433) !important;
}
/* 137deg的渐变 */
.theme-linear-gradient-137{
  background-image: linear-gradient(137deg, #FF5433, #FF2266) !important;
}
/* 137deg的反向渐变 */
.theme-linear-gradient-137-reverse{
  background-image: linear-gradient(137deg, #FF2266, #FF5433) !important;
}