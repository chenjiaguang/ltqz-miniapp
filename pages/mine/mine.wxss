/* pages/mine/mine.wxss */
.page-mine{
  background: #F1F1F1;
  padding-bottom: 80rpx;
}
.user-info{
  display: flex;
  height: 300rpx;
  background: green;
}
.user-person{
  margin-right: 20rpx;
}
.person-age{
  border-left: 1px solid #333;
}

.other-list{
  width: 88%;
  margin: -60rpx auto 0;
}
.order-box{
  background: #fff;
}
.order-header{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.order-type{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.type-item{
  width: 25%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.type-icon{
  width: 110rpx;
  height: 110rpx;
}

.profit-box{
  background: #fff;
  margin-top: 30rpx;
}
.profit-header{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.profit-overview{
  display: flex;
  justify-content: center;
  align-items: center;
}
.profit-overview-item{
  width: 50%;
  box-sizing: border-box;
  border-left: 1px solid #999;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.profit-overview-item:first-child{
  border-left: none;
}

.be-partner{
  background: #fff;
  margin-top: 30rpx;
}

.business-entrance{
  background: #fff;
  margin-top: 30rpx;
}

.other-entrances{
  background: #fff;
  margin-top: 30rpx;
}